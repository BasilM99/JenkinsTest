@model IList<ArabyAds.AdFalcon.Services.Interfaces.DTOs.Campaign.Creative.AdCreativeUnitDto> 
@foreach (var docId in Model)
{
    <div class="data-row info-container last-container">
        <div class="data-row data-container">
            <div class="label-container">
                <span class="field-title required-field reverse-link">@(docId.Name)</span>
            </div>
            <div class="field-container">
                <img   src='@(ArabyAds.AdFalcon.Web.Controllers.Utilities.Config.RootUrl + "Downloader.ashx?docId=" + @docId.DocumentId.ToString())' />
            </div>
        </div>
    </div>
}