{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },






  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:5005"
      },
      "Http": {
        "Url": "http://localhost:5050"
      }
    },
    "AllowSynchronousIO": true,
    "AddServerHeader": false,
    "Limits": {
      "MinRequestBodyDataRate": null,
      "MinResponseDataRate": null
    }
  }
}
