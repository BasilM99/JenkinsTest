@using Combres.Mvc
@using Noqoush.AdFalcon.Web.Controllers.Model;
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico">
    @(Html.Telerik().StyleSheetRegistrar().DefaultGroup(group => group.Add("/" + Noqoush.AdFalcon.Web.Controllers.Utilities.Config.CurrentLanguage + "/telerik.common.css").Add("/" + Noqoush.AdFalcon.Web.Controllers.Utilities.Config.CurrentLanguage + "/telerik.webblue.css").Add("/" + Noqoush.AdFalcon.Web.Controllers.Utilities.Config.CurrentLanguage + "/telerik.rtl.css").Combined(true).Compress(true)))
    @Html.RegisterInternalStyleSheetLink("style.less")
    @{
        if (Noqoush.AdFalcon.Web.Controllers.Utilities.Config.CurrentLanguage == "ar")
        {
        <!--[if IE 7 ]>@Html.RegisterInternalStyleSheetLink("styleie7.less")<![endif]-->
        }
    }
    @Html.RegisterInternalStyleSheetLink("ui.daterangepicker.css")
    @Html.RegisterInternalStyleSheetLink("jquery-ui-1.8.16.custom.css")
    @Html.RegisterInternalScriptLink("jquery-1.5.1.min.js")
    <script type="text/javascript">
    @Html.Partial("Localization")
    @{
        if ((ViewBag.isDefaultGoogleAnalytics == null) || (!ViewBag.isDefaultGoogleAnalytics))
        {
        <text>
            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-25905897-1']);
            _gaq.push(['_setDomainName', 'adfalcon.com']);
            _gaq.push(['_trackPageLoadTime']);
            _gaq.push(['_trackPageview']);

            (function() {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
</text>
          
        }
        }
    </script>
</head>
<body>
     @{
        if (Noqoush.AdFalcon.Web.Controllers.Utilities.Config.CurrentLanguage == "ar")
        {
        <text> <div class="t-rtl">
        </text>
        }
        else
        {
        <text> <div>
        </text>
        }
    }
        <div class="data-row header-container">
            <div class="center-piece">
                <a href="@Url.Action("index",
    "default")" title="Go to Home" class="company-logo">
                    <img src='@Url.GetImagePath("adfalcon-bg.png")'
    alt="Ad Falcon Icon" /></a>
                <div class="main-navigation">
                    @RenderPage("~/Views/Shared/UserInformation.cshtml")
                    @RenderPage("~/Views/Shared/Menu.cshtml")
                    <div class="data-row pink-seperator">
                    </div>
                    <div class=" data-row bread-crumbs-menu-container">
                        <table cellspacing="4" class="bc-conatiner">
                            <tr>
                                <td>
                                    <a href='@Url.Action("index", "Default")' title='@Html.GetResource("Home",
    "Global")' class="home-link"></a>
                                </td>
                                @if (ViewData["BreadCrumbLinks"] != null)
                                {
                                    List<BreadCrumbModel> links = ViewData["BreadCrumbLinks"] as List<BreadCrumbModel>;
                                    int i = 1; if (links != null)
                                    {
                                        foreach (BreadCrumbModel link in links.OrderBy(p
                                            => p.Order).ToList())
                                        {
                                            if (i != links.Count)
                                            { <td>
                                                            <a href="@link.Url">@link.Text</a>
                                    </td>
                                            }
                                            else
                                            { <td>
                                                            <span>@link.Text</span>
                                    </td> } i++;
                                        }
                                    }
                                }
                                else { }
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="page">
            <div id="header">
                <div id="title">
                </div>
            </div>
            <div id="main">
                @RenderBody()
                <div id="footer">
                </div>
            </div>
        </div>
        <div class="spinner-container" style="display: none">
            <img src="@Url.GetImagePath("ajax-loader.gif")"
    alt="">
        </div>
        <div id="dialog-confirm" title="?" style="display: none;">
            <p>
                <span class="ui-icon ui-icon-alert" style="float: left; margin: 0 7px 20px 0;"></span>
                <span id="dialog-confirm-Msg">@Html.GetResource("ProceedMsg") </span>
            </p>
        </div>
        @RenderPage("~/Views/Shared/Footer.cshtml") @* @Html.RegisterInternalScriptLink("jquery-1.5.1.min.js")
    @(Html.Telerik().ScriptRegistrar().DefaultGroup(group=> group.Combined(true).Compress(true)))
    @Html.RegisterInternalScriptLink("daterangepicker.jQuery.js") @Html.RegisterInternalScriptLink("jquery.validate.js")
    @Html.RegisterInternalScriptLink("jquery.validate.unobtrusive.js") @Html.RegisterInternalScriptLink("less-1.1.0.min.js")
    @Html.RegisterInternalScriptLink("jquery-ui-1.8.11.min.js") @Html.RegisterInternalScriptLink("jquery-actions.js")
    @Html.RegisterInternalScriptLink("jquery.ui.datepicker-Localization.js") @Html.RegisterInternalScriptLink("jquery.tree.js")
    @Html.RegisterInternalScriptLink("jquery.tree.checkbox.js") @Html.RegisterInternalScriptLink("jquery.json-2.2.min.js")
    @Html.RegisterInternalScriptLink("telerik.extensions.js") @Html.RegisterInternalScriptLink("AdFalcon.js")*@
        @* <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-1.5.1.min.js" type="text/javascript"></script>
    @(Html.Telerik().ScriptRegistrar().DefaultGroup(group => group.Combined(true).Compress(true)).jQuery(false).jQueryValidation(false))
    @Html.RegisterInternalScriptLink("daterangepicker.jQuery.js") <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.8/jquery.validate.min.js"
    type="text/javascript"></script> <script src="https://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.validate.unobtrusive.min.js"
    type="text/javascript"></script> <script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.8.11/jquery-ui.min.js"
    type="text/javascript"></script> @Html.RegisterInternalScriptLink("jquery-actions.js")
    @Html.RegisterInternalScriptLink("jquery.ui.datepicker-Localization.js") @Html.RegisterInternalScriptLink("jquery.tree.js")
    @Html.RegisterInternalScriptLink("jquery.tree.checkbox.js") @Html.RegisterInternalScriptLink("jquery.json-2.2.min.js")
    @Html.RegisterInternalScriptLink("telerik.extensions.js") @Html.RegisterInternalScriptLink("AdFalcon.js")
    @Html.CombresLink("siteJs")*@
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-1.5.1.min.js" type="text/javascript"></script>
        @(Html.Telerik().ScriptRegistrar().DefaultGroup(group
    => group.Combined(true).Compress(true)).jQuery(false).jQueryValidation(false))
        <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.8/jquery.validate.min.js"
            type="text/javascript"></script>
        <script src="https://ajax.aspnetcdn.com/ajax/mvc/3.0/jquery.validate.unobtrusive.min.js"
            type="text/javascript"></script>
        <script src="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.8.11/jquery-ui.min.js" type="text/javascript"></script>
        @Html.CombresLink("siteJs")
    </div>
</body>
</html>
